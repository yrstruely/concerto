{
  "name": "concerto-template",
  "version": "1.2.2",
  "description": "Server-side functional and performance test framework",
  "main": "index.js",
  "scripts": {
    "test": "mocha --sort=true --recursive --ignore=**/*.perf.js --require mochawesome/register --reporter mocha-multi-reporters --reporter-options configFile=./results/config.json",
    "perf-test": "k6 run  --http-debug=full ./clients/k6-client.js",
    "generate-k6-html-report": "node ./k6-html-reporter.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 index.js"
  },
  "author": "Kerry Harris",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "fs.promises": "^0.1.2",
    "glob": "^7.2.0",
    "k6": "0.0.0",
    "k6-html-reporter": "^1.0.5",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.0.1",
    "nodemon": "^2.0.15",
    "socks-proxy-agent": "^6.1.0",
    "tunnel": "0.0.6",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "axios-cookiejar-support": "^2.0.3",
    "cheerio": "^1.0.0-rc.10",
    "https": "^1.0.0",
    "moment-timezone": "^0.5.34",
    "qs": "^6.10.3",
    "tough-cookie": "^4.0.0"
  }
}

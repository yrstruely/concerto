{
  "name": "@yrstruely/concerto",
  "version": "2.0.33",
  "description": "Server-side functional and performance test framework",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha --sort=true --recursive --ignore=**/*.perf.js --ignore=**/*.ignore.js --require mochawesome/register --reporter mocha-multi-reporters --reporter-options configFile=./results/config.json",
    "generate-k6-config": "node ./helpers/k6/k6-config-generator.js",
    "perf-test": "k6 run ./client-configs/k6-config.js",
    "perf-test-local": "k6 run  --http-debug=full ./client-configs/k6-config.js",
    "generate-k6-html-report": "node ./helpers/k6/k6-html-reporter.js",
    "debug": "nodemon --inspect=0.0.0.0:9229 index.js",
    "npm-update": "npm install -g npm@latest",
    "npm-audit-fix": "npm audit fix"
  },
  "author": "Kerry Harris",
  "license": "ISC",
  "bin": {
    "concerto": "./cli.js"
  },
  "dependencies": {
    "@yrstruely/concerto": "^2.0.1",
    "ajv": "^8.10.0",
    "axios": "^0.24.0",
    "axios-cookiejar-support": "^2.0.3",
    "chai": "^4.3.4",
    "cheerio": "^1.0.0-rc.10",
    "commander": "^10.0.1",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "fs.promises": "^0.1.2",
    "get-stream": "^6.0.1",
    "glob": "^7.2.3",
    "https": "^1.0.0",
    "k6": "0.0.0",
    "k6-html-reporter": "^1.0.5",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.0.1",
    "moment-timezone": "^0.5.34",
    "nodemon": "^2.0.15",
    "pdfkit": "^0.13.0",
    "puppeteer": "^18.0.4",
    "qs": "^6.10.3",
    "socks-proxy-agent": "^6.1.0",
    "tough-cookie": "^4.0.0",
    "tunnel": "0.0.6",
    "uuid": "^8.3.2"
  }
}
